{
  "name": "react-template",
  "private": true,
  "version": "0.1.0-beta-0",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "20.18.2"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint \"{src,packages}/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write src/",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "pre-commit-lint": "lint-staged",
    "dev": "vite --mode dev",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "build:dev": "tsc -b && vite build --mode dev",
    "build:test": "tsc -b && vite build --mode test",
    "build:prod": "tsc -b && vite build --mode prod",
    "preview": "vite preview --mode dev",
    "preview:test": "vite preview --mode test",
    "preview:prod": "vite preview --mode prod"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "axios": "^1.6.8",
    "crypto-js": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "tailwindcss": "^4.1.10",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.11",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "unplugin-auto-import": "^19.3.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}